<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.mredrock.cyxbs.freshman">

    <application
            android:name="com.mredrock.cyxbs.common.BaseApp"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".activity.ShowPictureActivity">
        </activity>
        <activity android:name=".activity.ShowWayActivity">
        </activity>
        <activity android:name=".activity.NecessaryGoodsActivity">
        </activity>
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.DataActivity"/>
        <activity android:name=".activity.GuideActivity"/>
        <activity android:name=".activity.CommunicateActivity"/>
    </application>

</manifest>